{
  "name": "@neonlabs/neon-client-transfer",
  "version": "3.0.0-beta-3",
  "description": "JavaScript client-side module for creating transfer transactions from Solana EVM to Neon EVM",
  "files": [
    "./dist/**/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "yarn test",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:lint": "eslint ./dist",
    "test:audit": "npm audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neonlabsorg/neon-client-transfer.git"
  },
  "keywords": [
    "neonlabs",
    "neon-evm",
    "erc20",
    "spl",
    "token-transfering"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neonlabsorg/neon-client-transfer/issues"
  },
  "homepage": "https://github.com/neonlabsorg/neon-client-transfer#readme",
  "dependencies": {
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.77.3",
    "big.js": "^6.2.1",
    "web3": "1.10.0",
    "web3-core": "1.10",
    "web3-eth-contract": "1.10.0"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.6",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/parser": "^5.43.0",
    "bs58": "^5.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "eslint": "8.27.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "2.7.1",
    "react": "18",
    "react-dom": "^18.2.0",
    "solc": "^0.8.23-fixed",
    "token-list": "https://github.com/neonlabsorg/token-list.git#v5.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
